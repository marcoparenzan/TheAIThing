{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WorkerServicesManager": {
    "workerNames": "mqttbroker;timers;opcua1;timeseries"
  },
  "workers": {
    "mqttbroker": {
      "retainServiceName": "retain0"
    },
    "opcua1": {
      "endpointUrl": "opc.tcp://localhost:50000",
      "username": "user",
      "password": "password",
      "nodes": "ns=4;i=15013|ns=3;s=FastUInt1",
      "messagingClient": "mqttclient-opcua1"
    },
    "timers": {
      "messagingClient": "mqttclient-timers"
    },
    "timeseries": {
      "messagingClient": "mqttclient-timeseries"
    }
  },
  "mqttCredentials": {
    "host": "localhost",
    "port": 2883,
    "username": "ValidUser",
    "password": "SecretPassword"
  },
  "mqttclient-opcua1": {
    "credentialsName": "mqttCredentials"
  },
  "mqttclient-timers": {
    "credentialsName": "mqttCredentials"
  },
  "mqttclient-timeseries": {
    "credentialsName": "mqttCredentials"
  }
}
